    "prettier": "prettier --write '**/*'",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "fmt": "npm run prettier && npm run lint-fix",
    "build": "tsc -p ./tsconfig.build.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HiromiShikata/auto-issue-resolver.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HiromiShikata/auto-issue-resolver/issues"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branches": [
      "main"
    ]
  },
  "homepage": "https://github.com/HiromiShikata/auto-issue-resolver#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "check-node-version": "^4.2.1",
    "commitlint": "^17.7.1",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-no-type-assertion": "^1.3.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.6.3",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.2",
    "semantic-release": "^23.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.1",
    "commander": "^12.0.0",
    "dotenv": "^16.3.1",
    "openai": "^4.1.0"
